{
  "name": "@spatialdata/vis",
  "version": "0.0.1",
  "description": "React visualisation components for SpatialData",
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
